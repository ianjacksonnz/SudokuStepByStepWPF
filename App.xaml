<Application x:Class="SodokuSolver2025.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SudokuStepByStep" xmlns:converters="clr-namespace:SudokuStepByStep.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:ZeroToEmptyStringConverter x:Key="ZeroToEmptyStringConverter"/>
        <converters:SudokuBorderThicknessConverter x:Key="SudokuBorderThicknessConverter"/>
        <converters:PossibleNumbersToStringConverter x:Key="PossibleNumbersToStringConverter"/>
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
    </Application.Resources>
</Application>
